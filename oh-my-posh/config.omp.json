{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "palette": {
    "white": "white",
    "primary": "blue",
    "success": "green",
    "idle": "cyan",
    "warning": "yellow",
    "danger": "red",
    "os": "p:white",
    "prompt": "p:primary",
    "transient_prompt": "p:green",
    "secondary_prompt": "p:cyan",
    "path": "p:primary",
    "battery_foreground": "p:warning",
    "battery_charging": "p:primary",
    "battery:discharging": "p:danger",
    "battery_full": "p:success",
    "git": "p:idle",
    "git_changed": "p:warning",
    "git_unsynced": "magenta",
    "git_ahead": "p:success",
    "git_behind": "p:primary",
    "session_user": "p:idle",
    "session_root": "p:danger",
    "python": "#4584b6",
    "node": "#8cc84b",
    "elixir": "#b4a2be"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "\u250c ",
          "foreground": "p:white"
        },
        {
          "type": "os",
          "style": "plain",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "foreground": "p:os",
          "properties": {
            "macos": "\uf179 ",
            "ubuntu": "\uf31b "
          }
        },
        {
          "type": "session",
          "style": "plain",
          "template": "{{ if .SSHSession }} {{ end }}{{ if .Root }}\uf0e7{{ else }}\ueb99{{ end }} ",
          "foreground_templates": [
            "{{ if .Root }}p:session_root{{ else }}p:session_user{{ end }}"
          ]
        },
        {
          "type": "path",
          "style": "plain",
          "template": "{{ .Path }}",
          "foreground": "p:path",
          "properties": {
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git_changed{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git_unsynced{{ end }}",
            "{{ if gt .Ahead 0 }}p:git_ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git_behind{{ end }}"
          ],
          "foreground": "p:git",
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true,
            "full_branch_path": true
          }
        }
      ]
    },
    {
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "type": "node",
          "style": "plain",
          "foreground": "p:node",
          "template": " \ue718 {{ .Full }} "
        },
        {
          "type": "elixir",
          "style": "plain",
          "foreground": "p:elixir",
          "template": "  {{ .Full }} "
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:python",
          "template": " \ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }} {{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground": "p:battery_foreground",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}p:battery_charging{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:battery:discharging{{end}}",
            "{{if eq \"Full\" .State.String}}p:battery_full{{end}}"
          ],
          "template": "{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }} ",
          "properties": {
            "discharging_icon": " ",
            "charging_icon": " ",
            "charged_icon": " "
          }
        }
      ],
      "type": "prompt"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "\u2514 ",
          "foreground": "p:white"
        },
        {
          "type": "text",
          "style": "plain",
          "template": "\u276f",
          "foreground": "p:prompt",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:danger{{ end }}"
          ]
        }
      ]
    }
  ],
  "transient_prompt": {
    "foreground": "p:transient_prompt",
    "foreground_templates": [
      "{{ if gt .Code 0 }}p:danger{{ end }}"
    ],
    "template": "  \u276f "
  },
  "secondary_prompt": {
    "foreground": "p:secondary_prompt",
    "template": "  \u276f\u276f "
  }
}